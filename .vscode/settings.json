{
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/dist": true
  },
  "cSpell.words": [
    "SUPERADMIN",
    "ansvote",
    "answervote",
    "answervotes",
    "downvotes",
    "questionvote",
    "quesvote",
    "upvotes"
  ]
}
